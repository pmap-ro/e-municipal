@model dynamic
@{
    ViewData["Title"] = "Equipe de Governo";
}

<div class="container">


    <ol class="breadcrumb">
        <li><a href="~/">Início</a></li>
        <li>Prefeitura</li>
        <li><a asp-area="" asp-controller="prefeitura" asp-action="equipedegoverno" class="active">Equipe de Governo</a></li>
    </ol>
</div>
<section id="meet-team">
    <div class="container">
        <div class="section-header">
            <h2 class="section-title text-center wow fadeInDown">Equipe de Governo</h2>
            <p class="text-center wow fadeInDown">Conheça a equipe de governo do munícipio de Alto Paraíso - RO <br />que tem como finalidade regrar e organizar o município.</p>
        </div>

        <div class="row">

        @foreach(dynamic item in Model.EquipeDeGoverno)
        {
            <div class="col-xs-offset-2 col-xs-8 col-sm-offset-0 col-sm-6 col-md-3 margin-bottom-20px">
                <div class="team-member wow fadeInUp" data-wow-duration="400ms" data-wow-delay="0ms">
                    <div class="team-img">
                        <img class="img-responsive" src="@item.Imagem" alt="">
                    </div>
                    <div class="team-info">
                        <h3>@item.Nome</h3>
                        <span>@item.Cargo</span>
                    </div>
                <p>@item.Endereco</p>
                </div>
            </div>
        }

        
        </div>
        <div class="divider"></div>
        
    </div>
</section><!--/#meet-team-->
